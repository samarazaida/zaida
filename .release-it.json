{
  "pkgFiles": ["package.json"],
  "requireUpstream": false,
  "increment": "conventional",
  "beforeChangelogCommand": "conventional-changelog -i CHANGELOG.md -s",
  "changelogCommand": "conventional-changelog | tail -n +3",
  "requireCleanWorkingDir": false,
  "safeBump": false,
  "github": {
    "release": true
  },
  "buildCommand": "npm run build"
}
